{
  "name": "@knotcity/http-request-signer",
  "version": "0.3.0",
  "description": "Library used to sign http request as defined in https://tools.ietf.org/html/draft-cavage-http-signatures-12",
  "repository": {
    "type": "git",
    "url": "https://github.com/knotcity/http-requests-signer"
  },
  "license": "ISC",
  "author": "Knot",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:mjs",
    "build:cjs": "tsc --outDir dist/cjs --module CommonJS",
    "build:mjs": "tsc --outDir dist/mjs --module es2020",
    "lint": "eslint --ext ts src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^12.20.41",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "typescript": "^4.5.4"
  }
}
